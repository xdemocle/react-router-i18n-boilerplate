# EditorConfig helps maintain consistent coding styles
# See https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
print_width = 120
semi = false
singleQuote = true
trailingComma = es5
trim_trailing_whitespace = true